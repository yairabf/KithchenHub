{
  "cli": {
    "appVersionSource": "remote"
  },
  "build": {
    "preview": {
      "distribution": "internal",
      "channel": "develop",
      "android": {
        "buildType": "apk"
      }
    },
    "production": {
      "distribution": "store",
      "channel": "main",
      "autoIncrement": true,
      "credentialsSource": "remote",
      "android": {
        "buildType": "app-bundle"
      }
    }
  }
}
