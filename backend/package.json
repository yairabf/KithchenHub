{
  "name": "kitchen-hub-api",
  "version": "0.0.1",
  "description": "Kitchen Hub Backend API",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "prisma": {
    "schema": "src/infrastructure/database/prisma/schema.prisma"
  },
  "scripts": {
    "build": "nest build --tsc -p tsconfig.build.json",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "npm run lint:check",
    "lint:check": "eslint \"{src,apps,libs,test}/**/*.ts\" --max-warnings=0",
    "lint:fix": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "test": "jest",
    "test:unit": "jest --testPathIgnorePatterns=\"rls\\\\.spec\\\\.ts$\"",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "prisma:generate": "prisma generate --schema=src/infrastructure/database/prisma/schema.prisma",
    "prisma:migrate": "prisma migrate dev --schema=src/infrastructure/database/prisma/schema.prisma",
    "prisma:deploy": "prisma migrate deploy --schema=src/infrastructure/database/prisma/schema.prisma",
    "prisma:studio": "prisma studio --schema=src/infrastructure/database/prisma/schema.prisma",
    "db:start": "docker-compose -f docker-compose.db.yml --env-file .env.db up -d",
    "db:stop": "docker-compose -f docker-compose.db.yml --env-file .env.db down",
    "db:logs": "docker-compose -f docker-compose.db.yml --env-file .env.db logs -f",
    "db:ps": "docker-compose -f docker-compose.db.yml --env-file .env.db ps",
    "db:seed": "ts-node -r dotenv/config scripts/seed-grocery-catalog.ts"
  },
  "dependencies": {
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^10.0.0",
    "@nestjs/jwt": "^11.0.2",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-fastify": "^10.0.0",
    "@nestjs/swagger": "^7.4.2",
    "@prisma/client": "^5.0.0",
    "@sentry/node": "^8.0.0",
    "@supabase/supabase-js": "^2.91.0",
    "bcrypt": "^6.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.3",
    "google-auth-library": "^10.5.0",
    "ms": "^2.1.3",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pino": "^9.0.0",
    "pino-http": "^10.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/bcrypt": "^6.0.0",
    "@types/jest": "^30.0.0",
    "@types/ms": "^2.1.0",
    "@types/node": "^20.3.1",
    "@types/passport-jwt": "^4.0.1",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "dotenv": "^17.2.3",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.5.0",
    "pino-pretty": "^11.0.0",
    "prettier": "^3.0.0",
    "prisma": "^5.0.0",
    "source-map-support": "^0.5.21",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
